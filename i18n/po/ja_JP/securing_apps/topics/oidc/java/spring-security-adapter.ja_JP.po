"<dependency>\n"
"    <groupId>org.keycloak</groupId>\n"
"    <artifactId>keycloak-spring-security-adapter</artifactId>\n"
"    <version>{project_versionMvn}</version>\n"
"</dependency>\n"
msgstr ""
"<dependency>\n"
"    <groupId>org.keycloak</groupId>\n"
"    <artifactId>keycloak-spring-security-adapter</artifactId>\n"
"    <version>{project_versionMvn}</version>\n"
