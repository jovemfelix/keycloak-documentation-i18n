"   \"connection-pool-size\" : 20,\n"
"   \"disable-trust-manager\": false,\n"
"   \"allow-any-hostname\" : false,\n"
"   \"truststore\" : \"path/to/truststore.jks\",\n"
"   \"truststore-password\" : \"geheim\",\n"
