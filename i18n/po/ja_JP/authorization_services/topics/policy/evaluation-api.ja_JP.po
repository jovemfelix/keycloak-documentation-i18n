msgstr "    }\n"

#. type: Plain text
#, no-wrap
msgid "Returns the {@link ResourcePermission} to be evaluated.\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "*\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "@return the permission to be evaluated\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "*/\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "ResourcePermission getPermission();\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "/**\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "Returns the {@link EvaluationContext}. Which provides access to the whole evaluation runtime context.\n"
