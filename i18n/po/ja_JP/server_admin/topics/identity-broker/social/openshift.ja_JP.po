#| msgid "Creating and Registering the Client"
msgid "Registering OAuth client"
msgstr "クライアントの作成と登録"

#. type: Plain text
msgid "You can register your client using `oc` command line tool."
msgstr ""

#. type: delimited block -
#, no-wrap
msgid ""
"$ oc create -f <(echo '\n"
"kind: OAuthClient\n"
"apiVersion: v1\n"
"metadata:\n"
" name: kc-client <1>\n"
"secret: \"...\" <2>\n"
"redirectURIs:\n"
" - \"http://www.example.com/\" <3>\n"
"grantMethod: prompt <4>\n"
"')\n"
msgstr ""

#. type: Plain text
msgid "The `name` of your OAuth client. Passed as `client_id` request parameter when making requests to `_<openshift_master>_/oauth/authorize` and `_<openshift_master>_/oauth/token`."
msgstr ""

#. type: Plain text
msgid "`secret` is used as the `client_secret` request parameter."
msgstr ""

#. type: Plain text
msgid "The `redirect_uri` parameter specified in requests to `_<openshift_master>_/oauth/authorize` and `_<openshift_master>_/oauth/token` must be equal to (or prefixed by) one of the URIs in `redirectURIs`."
msgstr ""

#. type: Plain text
msgid "The `grantMethod` is used to determine what action to take when this client requests tokens and has not yet been granted access by the user."
msgstr ""

#. type: delimited block =
msgid "Use client ID and secret defined by `oc create` command to enter them back on the {project_name} `Add identity provider` page.  Go back to {project_name} and specify those items."
msgstr ""

